{
  "name": "Priva",
  "description": "A Google Chrome extension!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "shield.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.snap.com/*"],
      "js": ["content-script.js"]
    }
  ]
}